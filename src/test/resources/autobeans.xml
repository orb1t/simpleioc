<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<beans xmlns="http://org.om.dynabean">
	<bean name="jcrsessionfactory" autocreate="true" cache="true">
		<class>java.io.File</class>
		<arguments>
			<argument>
				<value>target/repository</value>
			</argument>
		</arguments>
	</bean>
	<bean name="omfactory" autocreate="false" cache="false">
		<class>java.io.File</class>
		<arguments>
			<argument reference="true">
				<value>strstr</value>
			</argument>
		</arguments>
	</bean>
	<bean name="strstr" autocreate="false" cache="false">
		<class>java.lang.String</class>
		<arguments>
			<argument>
				<value>target/repository</value>
			</argument>
		</arguments>
	</bean>
</beans>

